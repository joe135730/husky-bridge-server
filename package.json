{
  "type": "module",
  "name": "husky-bridge-server",
  "version": "1.0.0",
  "description": "Node.js HTTP Web server for husky-bridge",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:api": "newman run postman/Husky-Bridge-API.postman_collection.json -e postman/Husky-Bridge-API.postman_environment.json --reporters cli,json --reporter-json-export postman/results.json",
    "start": "node index.js",
    "lint": "eslint .",
    "build": "echo \"No build step needed for Node.js server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe135730/husky-bridge-server.git"
  },
  "keywords": [
    "Node",
    "REST",
    "Server",
    "HTTP",
    "Web",
    "Development"
  ],
  "author": "Che-Yi Wu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joe135730/husky-bridge-server/issues"
  },
  "homepage": "https://github.com/joe135730/husky-bridge-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "mongoose": "^8.13.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "newman": "^6.1.0",
    "vitest": "^4.0.14"
  }
}
